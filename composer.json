{
    "name": "GalenCollege/bootstrap-ui",
    "description": "Twitter Bootstrap 3 support for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "twitter", "bootstrap"],
    "homepage": "http://github.com/GalenCollege/bootstrap-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Howatt",
            "homepage": "http://galencollege.edu",
            "role": "Author"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "support": {
        "issues": "http://github.com/GalenCollege/bootstrap-ui/issues",
        "source": "http://github.com/GalenCollege/bootstrap-ui"
    },
    "autoload": {
        "psr-4": {
            "BootstrapUI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BootstrapUI\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
